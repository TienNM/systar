<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<NetTransceiver>
	<NetServices>
		<NetService name="socket" class="com.soyomaker.net.mina.MinaServer"
			port="8080" readBufferSize="100000" />
	</NetServices>
	<protocols>
		<protocol id="101001" NetService="socket"
			handler="com.soyomaker.handlers.login.RegisterHandler" needLogin="false" />
		<protocol id="101002" NetService="socket"
			handler="com.soyomaker.handlers.login.LoginHandler" needLogin="false" />
	</protocols>
</NetTransceiver>
